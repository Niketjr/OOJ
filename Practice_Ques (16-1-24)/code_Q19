abstract class Bird{
    abstract void fly();
    abstract void makeSound();
}
class eagle extends Bird{
    void fly(){
        System.out.println("FLies like an eagle");
    }
    void makeSound(){
        System.out.println("CHEE..?");
    }
}
class hawk extends Bird
{
    void fly(){
        System.out.println("FLies like an eagle but more like hawk");
    }
    void makeSound(){
        System.out.println("KAW..?");
    }
}
class HelloWorld {
    public static void main(String[] args) {
        hawk h = new hawk();
        h.fly();
        h.makeSound();
        eagle e = new eagle();
        e.fly();
        e.makeSound();
    }
}
